{
  "compilerOptions": {
    "outDir": "./",
    "module": "ES2022",
    "target": "ES2022",
    "lib": ["DOM","ES2022"],
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "node",
    "rootDir": "./",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": false,
    "suppressImplicitAnyIndexErrors": true,
    "experimentalDecorators": true,
    "types": [
      "node",
      "webpack-env"
    ]
  },
  "files": [
    "Empresa.ts",
    "Candidato.ts",
    "Usuario.ts",
    "scriptCadastroCandidatos.ts",
    "scriptCadastroEmpresas.ts",
    "scriptPerfilEmpresa.ts",
    "scriptPerfilCandidato.ts",
  ]
}